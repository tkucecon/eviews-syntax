{
  "name": "eviews-syntax",
  "displayName": "Eviews Syntax",
  "description": "Provides Eviews Syntax highlights for VSCode",
  "version": "0.0.1",
  "engines": {
    "vscode": "^1.97.0"
  },
  "categories": [
    "Programming Languages"
  ],
  "contributes": {
    "languages": [{
      "id": "eviews",
      "aliases": ["PRG", "prg"],
      "extensions": [".prg"],
      "configuration": "./language-configuration.json"
    }],
    "grammars": [{
      "language": "eviews",
      "scopeName": "source.eviews",
      "path": "./syntaxes/prg.tmLanguage.json"
    }]
  }
}
